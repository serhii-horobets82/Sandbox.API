{
  "JwtIssuerOptions": {
    "Issuer": "EvoflareApi",
    "Audience": "https://evoflareapi.azurewebsites.net"
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "example.com"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          // TODO
        }
      },
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
